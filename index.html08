    async function getWeather() {
        const city = document.getElementById('city').value;
        if(!city || city.includes('...')) return;
        try {
            const r1 = await fetch(`https://api.openweathermap.org/data/2.5/weather?q=${city},IR&appid=${API}&units=metric&lang=fa`);
            const d1 = await r1.json();
            const r2 = await fetch(`https://api.openweathermap.org/data/2.5/forecast?q=${city},IR&appid=${API}&units=metric&lang=fa`);
            const d2 = await r2.json();
            currentData = d1;
            render(d1, d2.list);
        } catch (e) { alert("Ø®Ø·Ø§ Ø¯Ø± Ø¯Ø±ÛŒØ§ÙØª Ø§Ø·Ù„Ø§Ø¹Ø§Øª. Ù†Ø§Ù… Ø´Ù‡Ø± Ø±Ø§ Ø¨Ø±Ø±Ø³ÛŒ Ú©Ù†ÛŒØ¯."); }
    }

    function render(d, f) {
        document.getElementById('result').style.display = 'block';
        document.getElementById('res-city').innerText = d.name;
        document.getElementById('res-temp').innerText = toFa(Math.round(d.main.temp)) + "Â°C";
        document.getElementById('res-desc').innerText = d.weather[0].description;
        document.getElementById('res-icon').innerHTML = `<img src="https://openweathermap.org/img/wn/${d.weather[0].icon}@4x.png" width="100">`;
        
        document.getElementById('wind').innerText = toFa((d.wind.speed * 3.6).toFixed(1)) + " km/h";
        document.getElementById('hum').innerText = toFa(d.main.humidity) + "%";
        document.getElementById('pres').innerText = toFa(d.main.pressure) + " mb";
        document.getElementById('cloud').innerText = toFa(d.clouds.all) + "%";

        const box = document.getElementById('forecast');
        box.innerHTML = '';
        const seen = [];
        f.forEach(item => {
            const day = new Date(item.dt * 1000).toLocaleDateString('fa-IR', {weekday:'long'});
            if(!seen.includes(day) && seen.length < 5) {
                seen.push(day);
                box.innerHTML += `<div class="f-item">
                    <div style="font-size:0.7rem">${day}</div>
                    <img src="https://openweathermap.org/img/wn/${item.weather[0].icon}.png">
                    <div style="font-weight:900">${toFa(Math.round(item.main.temp))}Â°</div>
                </div>`;
            }
        });
    }

