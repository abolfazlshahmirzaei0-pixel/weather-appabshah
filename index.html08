    function addComment() {
        const t = document.getElementById('c-input').value;
        if(!t) return;
        const list = JSON.parse(localStorage.getItem('cm') || '[]');
        list.unshift({u: localStorage.getItem('u'), t, d: new Date().toLocaleTimeString('fa-IR')});
        localStorage.setItem('cm', JSON.stringify(list.slice(0,15)));
        document.getElementById('c-input').value = '';
        loadComments();
    }

    function loadComments() {
        const list = JSON.parse(localStorage.getItem('cm') || '[]');
        document.getElementById('c-list').innerHTML = list.map(i => `<div class="comment-item"><b>${i.u}:</b> ${i.t} <small style="float:left; opacity:0.5">${i.d}</small></div>`).join('');
    }

